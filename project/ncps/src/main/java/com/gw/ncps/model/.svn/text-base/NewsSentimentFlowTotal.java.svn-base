package com.gw.ncps.model;

import org.apache.ibatis.type.Alias;

/**
 * @function：情感流入、流出量类
 * @author Darwen
 * @data:2013-10-14下午3:41:19
 *
 */
@Alias("NewsSentimentFlowTotal")
public class NewsSentimentFlowTotal {
	
	private long newsID;  //王那边自增ID
	private long newsCode; // 新闻ID
	private String timeIn; // 流入时间
	private String timeOut; // 流入时间
	
	public long getNewsCode() {
		return newsCode;
	}
	public void setNewsCode(long newsCode) {
		this.newsCode = newsCode;
	}
	public String getTimeIn() {
		return timeIn;
	}
	public void setTimeIn(String timeIn) {
		this.timeIn = timeIn;
	}
	public String getTimeOut() {
		return timeOut;
	}
	public void setTimeOut(String timeOut) {
		this.timeOut = timeOut;
	}
	public long getNewsID() {
		return newsID;
	}
	public void setNewsID(long newsID) {
		this.newsID = newsID;
	}
	@Override
	public String toString() {
		return "NewsSentimentFlowTotal [newsID=" + newsID + ", newsCode="
				+ newsCode + ", timeIn=" + timeIn + ", timeOut=" + timeOut
				+ "]";
	}
	

}
